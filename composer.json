{
    "name": "riesenia/persist-related-data",
    "description": "CakePHP ORM plugin for persisting selected fields of related tables",
    "keywords": ["cakephp", "orm", "related", "materialize"],
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomas Saghy",
            "email": "segy@riesenia.com"
        }
    ],
    "require": {
        "cakephp/orm": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0",
        "cakephp/cakephp": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "PersistRelatedData\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PersistRelatedData\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
