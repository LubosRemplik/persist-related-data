{
    "name": "riesenia/persist-related-data",
    "description": "CakePHP plugin for persisting selected fields of related tables",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomas Saghy",
            "email": "segy@riesenia.com"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/orm": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "PersistRelatedData\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PersistRelatedData\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
